{
  "functions": {
    "api/index.ts": {
      "includeFiles": "./src/**/*.graphql"
    }
  },
  "rewrites": [
    {"source": "/", "destination": "/api/index"},
    {"source": "/stylesheets/:match*", "destination": "/docs/stylesheets/:match*"},
    {"source": "/javascripts/:match*", "destination": "/docs/javascripts/:match*"}
  ]
}
